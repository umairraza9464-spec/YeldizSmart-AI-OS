# YeldizSmart AI Environment Variables

# Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# Google Sheets Webhook
GOOGLE_SHEETS_WEBHOOK_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Node.js Backend
PORT=3000
NODE_ENV=development

# Playwright
HEADLESS=true
BROWSER_TIMEOUT=30000

# Proxy Configuration
PROXY_RESIDENTIAL=http://user:pass@proxy-residential:port
PROXY_MOBILE=http://user:pass@proxy-mobile:port

# Database
DB_PATH=data/yeldizsmart.db

# Timezone
TIMEZONE=Asia/Kolkata

# Work Window (IST)
WORK_START=04:00
WORK_END=12:00

# Logging
LOG_LEVEL=info

# Cities (comma-separated)
CITIES=Mumbai,Delhi,Pune,Bangalore,Hyderabad
